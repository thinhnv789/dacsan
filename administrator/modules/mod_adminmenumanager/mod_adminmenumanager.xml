	<config>
		<fields name="params">
			<fieldset name="basic">
				<field
					name="adminmenumanagermenu"
					type="adminmenumanagermenu"
					label="Menu"					
					addfieldpath="/administrator/modules/mod_adminmenumanager/params"
				/>	
				<field
					name="adminmenumanagermodpos"
					type="adminmenumanagermodpos"
					label="MOD_ADMINMENUMANAGER_PUBLISH_TO"					
					addfieldpath="/administrator/modules/mod_adminmenumanager/params"
				/>					
				<field
					name="allow_left"
					default="1"
					type="adminmenumanagerleft"
					label="MOD_ADMINMENUMANAGER_NO_MODULE_LEFT"	
					description="MOD_ADMINMENUMANAGER_NO_MODULE_LEFT_DESC"		
					addfieldpath="/administrator/modules/mod_adminmenumanager/params"
				/>					
				<field
					name="allow_right"
					default="1"
					type="adminmenumanagerright"
					label="MOD_ADMINMENUMANAGER_NO_MODULE_RIGHT"	
					description="MOD_ADMINMENUMANAGER_NO_MODULE_RIGHT_DESC"		
					addfieldpath="/administrator/modules/mod_adminmenumanager/params"
				/>	
				<field
					name="ammstyle"					
					type="adminmenumanagerstyle"
					default="auto"
					label="MOD_ADMINMENUMANAGER_STYLE"							
					addfieldpath="/administrator/modules/mod_adminmenumanager/params"
				/>	
				<field
					name="extracss"
					type="textarea"					
					label="MOD_ADMINMENUMANAGER_ADDITIONAL_CSS">										
				</field>
				<field
					name="adminmenumanagerdisable"					
					type="adminmenumanagerdisable"
					default="1"
					label="MOD_ADMINMENUMANAGER_WHEN_EDIT_MODE"	
					description="MOD_ADMINMENUMANAGER_WHEN_EDIT_MODE_DESC"								
					addfieldpath="/administrator/modules/mod_adminmenumanager/params"
				/>	
				<field
					name="adminmenumanagercursor"					
					type="adminmenumanagercursor"
					default="arrow"
					label="MOD_ADMINMENUMANAGER_CURSOR_STYLE"	
					description="MOD_ADMINMENUMANAGER_CURSOR_STYLE_DESC"								
					addfieldpath="/administrator/modules/mod_adminmenumanager/params"
				/>									
			</fieldset>	
			<fieldset name="advanced">	
				<field
					name="class_sfx"
					type="text"
					label="COM_MODULES_FIELD_MODULECLASS_SFX_LABEL"
					description="COM_MODULES_FIELD_MODULECLASS_SFX_DESC" 
				/>
